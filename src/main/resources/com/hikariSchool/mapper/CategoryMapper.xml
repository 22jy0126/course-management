<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hikariSchool.mapper.CategoryMapper">
	<insert id="saveNew">
		INSERT INTO it_category (categoryId, categoryName) VALUES(
			#{categoryId},
			#{categoryName}
		);
	</insert>
	<select id="getAll">
		select categoryId, categoryName from it_category;
	</select>
</mapper>