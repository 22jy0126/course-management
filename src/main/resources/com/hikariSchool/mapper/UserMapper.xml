<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hikariSchool.mapper.UserMapper">
	<select id="findUser">
		select * from it_user where userName = #{userName} and password = #{password};
	</select>
</mapper>